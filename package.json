{
  "name": "sqltools-athena-driver",
  "displayName": "SQLTools AWS Athena Driver",
  "description": "SQLTools AWS Athena Driver",
  "version": "0.3.6",
  "publisher": "pomaretta",
  "license": "MIT",
  "icon": "icons/extension_icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/pomaretta/sqltools-athena-driver"
  },
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Formatters",
    "Other"
  ],
  "keywords": [
    "athena",
    "sqltools-driver",
    "sqltools",
    "driver"
  ],
  "activationEvents": [
    "*",
    "onLanguage:sql",
    "onCommand:sqltools.*"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "tsc -p ./ && npm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=dist/extension.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "test-compile": "tsc -p ./",
    "vscode:package": "vsce package --allow-star-activation -o ./dist"
  },
  "dependencies": {
    "@sqltools/base-driver": "^0.1.11",
    "@sqltools/types": "latest",
    "aws-sdk": "^2.869.0",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.5.5",
    "@types/node": "18.x",
    "@types/vscode": "^1.42.0",
    "esbuild": "^0.21.4"
  },
  "extensionDependencies": [
    "mtxr.sqltools"
  ]
}
